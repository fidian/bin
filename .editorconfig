# EditorConfig, so you don't do it yourself - http://editorconfig.org

root = true

# Unix style newlines and a trailing newline for each file
# Plus, I am fond of tabs
[*]
end_of_file = lf
insert_final_newline = true
indent_style = space
indent_size = 9
tab_width = 3
charset = utf-8
trim_trailing_whitespace = true
